  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1",
    "sass": "^1.32.0"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node": "^21.6.2",
    "nodemailer": "^6.9.10",
    "xss": "^1.0.14"
  }
}
